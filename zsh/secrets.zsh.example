# ~/.oh-my-zsh/custom/secrets.zsh

# template for secrets.zsh
# copy this file to secrets.zsh and add your sensitive information
# secrets.zsh is ignored by git (listed in .gitignore)

# API keys
export OPENAI_API_KEY=""
export GITHUB_TOKEN=""
export AWS_ACCESS_KEY_ID=""
export AWS_SECRET_ACCESS_KEY=""
export AWS_DEFAULT_REGION=""

# database credentials
export DB_HOST=""
export DB_PORT=""
export DB_USER=""
export DB_PASSWORD=
export DB_NAME=""

# other services
export SLACK_TOKEN=""
export DOCKER_TOKEN=""
export NPM_TOKEN=""

# custom environment variables
export CUSTOM_API_KEY=""
export CUSTOM_API_SECRET=""

# development settings
export DEV_DATABASE_URL=""
export STAGING_DATABASE_URL=""
export PRODUCTION_DATABASE_URL=""
